{
  "gitSiteUrl": "https://github.com/fltsci/tauri-plugin-tracing/",
  "pkgManagers": {
    "javascript": {
      "version": true,
      "getPublishedVersion": {
        "use": "fetch:check",
        "options": {
          "url": "https://registry.npmjs.com/${ pkg.pkgFile.pkg.name }/${ pkg.pkgFile.version }"
        }
      },
      "publish": [
        {
          "command": "pnpm build",
          "dryRunCommand": "pnpm build"
        },
        {
          "command": "npm publish --access restricted --tag '${ pkgFile.pkg.version.split('-')[1].split('.')[0] ?? 'latest' }'",
          "dryRunCommand": "npm publish --access restricted --tag '${ pkgFile.pkg.version.split('-')[1].split('.')[0] ?? 'latest' }' --dry-run",
          "pipe": true
        }
      ],
      "postpublish": false
    },
    "rust": {
      "version": true,
      "getPublishedVersion": {
        "use": "fetch:check",
        "options": {
          "url": "https://crates.newearth.llc/api/v1/crates/${ pkg.pkgFile.pkg.package.name }/${ pkg.pkgFile.version }"
        }
      },
      "publish": [
      
        {
          "command": "cargo publish --allow-dirty --no-verify --registry kellnr",
          "dryRunCommand": "cargo publish --dry-run --registry kellnr",
          "pipe": true
        }
      
      ]
    }
  },
  "packages": {
    "tracing": {
      "path": ".",
      "manager": "rust"
    },
    "tracing-js": {
      "path": ".",
      "manager": "javascript"
    }
  }
}
